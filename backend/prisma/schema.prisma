// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model UserHelpX {
  id       String @id @default(uuid())
  nameUser String
  email    String @unique
  passwd   String

  @@map("users")
}

model Ticket {
  id       Int     @id @default(autoincrement())
  title    String
  resolved Boolean

  @@map("tickets")
}
